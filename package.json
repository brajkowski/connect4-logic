  "types": "dist/index.d.ts",
  "author": "Brandon Rajkowski",
  "repository": {
    "type": "git",
    "url": "https://github.com/brajkowski/connect4-logic.git"
  },
  "keywords": [
    "connect4",
    "logic",
    "ts",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {

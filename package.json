{
  "name": "@brajkowski/connect4-logic",
  "description": "This library provides APIs for manipulating and querying a standard (7x6) game of connect 4.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Brandon Rajkowski",
  "repository": {
    "type": "git",
    "url": "https://github.com/brajkowski/connect4-logic.git"
  },
  "keywords": [
    "connect4",
    "logic",
    "ts",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "build": "rimraf ./dist && tsc"
  },
  "jest": {
    "transform": {
      ".ts": "ts-jest"
    },
    "testRegex": "(/test/.*|\\.(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "jest": "^27.2.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "big-integer": "^1.6.48"
  }
}
